<h1>Users#show</h1>
<div>
	<%= @user.email %>
	<%= @user.first_name %>
	<%= @user.last_name %>

</div>
<div class="users_settings">
	 <ul class="nav nav-tabs" id="myTab" role="tablist">
	    <li class="nav-item">
	      <a class="nav-link active" data-toggle="tab" href="#not_ready" role="tab" aria-controls="not_ready">Preferances</a>
	    </li>
	  	<li class="nav-item">
	      <a class="nav-link" data-toggle="tab" href="#edit_profile" role="tab" aria-controls="edit_profile">Edit Profile</a>
	    </li>
	   </ul>
	  <div class="tab-content">
	    <div class="tab-pane active" id="not_ready" role="tabpanel">Preferances</div>
	    <div class="tab-pane" id="edit_profile" role="tabpanel"><%= render 'users/registrations/edit' %></div>
	  </div>
	  <script>
	    $(function () {
	      $('#myTab a:first').tab('show')
	    })
	  </script>

</div>


