<% @result.items.each do |item| %>
	<% if item.price %>
		<div>
			<button id="<%=item.name.gsub(/[^0-9A-Za-z]/, '')%>">
				<%= link_to "+", orders_path(user_id: @current_user_id, name: item.name, price: item.price, walmart_id: item.upc), method: :post, remote: true %>
			</button>	
			<span><%= item.name %> </span> <span>Price: <%= number_to_currency(item.price) %></span> 	
		</div>	
	<% end %>
<% end %>